---
layout: null
---
[
    {% for post in site.posts %
    }
    {
        "title": {
            { post.title | jsonify
            }
        },
        "url": {
            { post.url | relative_url | jsonify
            }
        },
        "date": "{{ post.date | date: "%b %-d, %Y" }}",
        "categories": {
            { post.categories | jsonify
            }
        },
        "content": {
            { post.content | strip_html | strip_newlines | jsonify
            }
        }
    }{% unless forloop.last %
    },
    {% endunless %
    }
    {% endfor %
    }
]